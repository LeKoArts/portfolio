{
  "name": "lekoarts-portfolio",
  "description": "Personal Portfolio of LekoArts",
  "repository": {
    "type": "git",
    "url": "https://github.com/LekoArts/portfolio"
  },
  "version": "2.0.0",
  "private": true,
  "author": "LekoArts <hello@lekoarts.de>",
  "scripts": {
    "dev": "gatsby develop -o",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build": "gatsby build",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y gatsby build",
    "serve": "gatsby serve",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_baseUrl=http://localhost:9000 cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "format": "prettier \"**/*.{md,mdx,yml} \" --write",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "serve-and-run": "start-server-and-test serve http://localhost:9000 cy:run",
    "test:e2e": "yarn build:cypress && yarn serve-and-run",
    "test:e2e:dev": "start-server-and-test develop:cypress http://localhost:8000 cy:open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "date-fns": "^2.7.0",
    "gatsby": "^2.17.15",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-catch-links": "^2.1.17",
    "gatsby-plugin-google-analytics": "^2.1.27",
    "gatsby-plugin-lodash": "^3.1.15",
    "gatsby-plugin-manifest": "^2.2.28",
    "gatsby-plugin-netlify": "^2.1.25",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "^3.0.21",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-sharp": "^2.3.0",
    "gatsby-plugin-sitemap": "^2.2.21",
    "gatsby-plugin-styled-components": "^3.1.13",
    "gatsby-plugin-typography": "^2.3.17",
    "gatsby-source-prismic": "^2.2.0",
    "gatsby-transformer-sharp": "^2.3.5",
    "lodash": "^4.17.15",
    "polished": "^3.4.2",
    "prismic-dom": "^2.1.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.11.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^5.2.1",
    "react-spring": "^8.0.27",
    "react-typography": "^0.16.19",
    "styled-components": "^4.4.1",
    "styled-theming": "^2.2.0",
    "typeface-istok-web": "^0.0.71",
    "typeface-montserrat": "^0.0.75",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^6.0.3",
    "cypress": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gatsby-cypress": "^0.2.16",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6"
  },
  "keywords": [
    "gatsby",
    "portfolio",
    "lekoarts",
    "prismic"
  ],
  "license": "UNLICENSED",
  "main": "n/a"
}
